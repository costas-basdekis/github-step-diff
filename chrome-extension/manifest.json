{
  "manifest_version": 2,

  "name": "Github step diff",
  "description": "Github commit-by-commit PR viewer",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "run_at": "document_end",
      "css": [
        "main.css"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "main.js"
      ]
    }
  ]
}
